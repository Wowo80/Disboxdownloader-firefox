{
    "name": "DisboxDownloader",
    "description": "Used to download files for Disbox.",
    "version": "1.0.0",
    "manifest_version": 2,
    "background": {
  "scripts": ["background.js"]
    },
    "permissions": [
        "*://*.discordapp.com/*",
        "*://*.discord.com/*",
        "*://disboxapp.github.io/*",
        "*://localhost/*"
    ],
    "browser_specific_settings": {
  "gecko": {
    "id": "disboxdwonloaderff@wowo.com",
    "strict_min_version": "109.0"
  }
}

}